{
	"manifest_version": 3,
	"name": "Dress Sense",
	"version": "1.0",
	"description": "Dress Sense is a Chrome extension that helps you to dress better.",
	"action": {
		"default_popup": "index.html",
		"default_title": "Dress Sense"
	},
	"permissions": ["activeTab", "scripting"],
	"host_permissions": ["<all_urls>"],
	"background": {
		"service_worker": "background.js"
	}
}
